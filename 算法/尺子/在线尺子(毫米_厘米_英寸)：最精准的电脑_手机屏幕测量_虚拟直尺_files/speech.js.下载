function say_the_length(x,y){
if ((window.ginisafe==0x7CF)&&('speechSynthesis' in window)){
let smp=document.getElementById('spanMousePosition'),oppi=document.getElementById('ppi');
if (smp==null||oppi==null){return;}
let ppi=parseFloat(oppi.value);
if (x<=0 || isNaN(ppi)){return;}
let txt='';
  if (y<65){//cm
    let cm=Math.round(x/ppi*25.4)/10;
    txt=s_hereis+' '+cm+' '+(cm==1?s_1cm:s_cms);
  }else{
  let inch=x/ppi,fractions=document.getElementById('fractions').value;
  let inch_txt=Decimal2Fraction(inch,fractions);
    inch_txt=inch_txt.replace(" ",s_and).replace("/",s_slash); 
    txt=s_hereis+' '+inch_txt + ' '+(inch_txt==1?s_1in:s_ins);
  }
  console.log(txt);
  try{
    speechSynthesis.cancel();
    if ((utterance.voice==null)&&(utterance.lang=='')) {
      utterance.lang=language_code;
      console.log(utterance.lang);
    }
    utterance.text=txt;
    speechSynthesis.speak(utterance);
  } catch(e){ console.log(e);}
}}

function show_mouse_position(x,y){
if (window.ginisafe==0x7CF){
let smp=document.getElementById('spanMousePosition'),oppi=document.getElementById('ppi');
if (smp==null||oppi==null){return;}
let ppi=parseFloat(oppi.value);
  if (x<0 || isNaN(ppi)){
    smp.innerHTML='';
  } else if (y<65){//cm
    let cm=Math.round(x/ppi*25.4)/10;
    smp.innerHTML=' , '+ cm +' '+ (cm==1?s_1cm:s_cms);
  }else{
    let inch=x/ppi,fractions=document.getElementById('fractions').value;
    let inch_txt=Decimal2Fraction(inch,fractions);
    smp.innerHTML=' , '+ inch_txt + ' '+ (inch_txt==1?s_1in:s_ins);
  }
}}