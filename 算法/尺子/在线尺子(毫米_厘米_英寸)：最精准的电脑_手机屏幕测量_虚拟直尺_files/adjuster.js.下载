document.addEventListener("DOMContentLoaded", function(){
let oadjuster=document.getElementById("adjuster");
if (typeof(oadjuster)=='undefined' || oadjuster == null){return;}  
var adjusters={name:[],inch:[],selectUI:oadjuster,
additem: function(name,inch) {
  this.name.push(name);
  this.inch.push(inch);
  var opt = document.createElement('option');
  opt.value=inch;
  opt.innerHTML=name;
  this.selectUI.appendChild(opt);
  }
};  

adjusters.additem("standard credit card (width/3.375in)",3.375);
adjusters.additem("standard credit card (height/2.215in)",2.125);
if (screen.width>700){adjusters.additem("2 credit cards width (6.75in)",6.75);}
if (screen.width>1000){adjusters.additem("3 credit cards width (10.125in)",10.125);}
if (screen.width>500){adjusters.additem("standard CD diameter (120mm)",4.724);}

var xhr = new XMLHttpRequest();

var jsonfile='https://ipapi.co/json/';
xhr.open('GET',jsonfile,true);
xhr.responseType = 'json';
xhr.onreadystatechange = function(e) {
  if (this.readyState == 4 && this.status == 200) {
  let c=this.response.country_code,sw=screen.width,oo=oadjuster.options;
